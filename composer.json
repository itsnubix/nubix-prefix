{
    "name": "itsnubix/nubix-preset",
    "description": "Nubix preset for Laravel (^8.0).",
    "license": "MIT",
    "keywords": [
        "tailwind",
        "tailwindcss",
        "tailwindui",
        "laravel",
        "breeze",
        "inertiajs",
        "eslint",
        "prettier",
        "preset",
        "vue",
        "vue3"
    ],
    "authors": [
        {
            "name": "Ron Yu",
            "email": "ryu@nubix.ca"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/ui": "^3.3",
        "laravel/breeze": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nubix\\Preset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nubix\\Preset\\PresetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
